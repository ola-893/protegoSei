{
  "name": "protego-ai-sei",
  "version": "1.0.0",
  "description": "Protego.ai - Revolutionary Invoice Financing Platform on Sei Network",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy-sei.js --network localhost",
    "deploy:testnet": "hardhat deploy-sei-testnet",
    "deploy:mainnet": "hardhat deploy-sei-mainnet --verify",
    "simulate": "hardhat run scripts/simulate-protego.js --network localhost",
    "simulate:testnet": "hardhat run scripts/simulate-protego.js --network seiTestnet",
    "verify:testnet": "hardhat run scripts/verify-contracts.js --network seiTestnet",
    "verify:mainnet": "hardhat run scripts/verify-contracts.js --network seiMainnet",
    "status": "hardhat sei-status",
    "estimate-costs": "hardhat estimate-costs",
    "fund-accounts": "hardhat fund-accounts",
    "size-contracts": "hardhat size-contracts",
    "gas-report": "REPORT_GAS=true hardhat test",
    "build": "npm run clean && npm run compile",
    "dev": "npm run build && npm run node",
    "demo": "npm run build && npm run deploy:local && npm run simulate",
    "lint": "prettier --write 'contracts/**/*.sol' && solhint 'contracts/**/*.sol'",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "defi",
    "invoice-financing",
    "sei-network",
    "erc721",
    "erc4626",
    "erc1155",
    "yield-farming",
    "protego-ai"
  ],
  "author": "Protego.ai Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.1",
    "chai": "^4.3.7",
    "ethers": "^5.8.0",
    "hardhat": "^2.17.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/protego-ai/sei-contracts.git"
  },
  "bugs": {
    "url": "https://github.com/protego-ai/sei-contracts/issues"
  },
  "homepage": "https://protego.ai",
  "lint-staged": {
    "*.sol": [
      "prettier --write",
      "solhint"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
